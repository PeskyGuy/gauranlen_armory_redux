<?xml version="1.0" encoding="utf-8" ?>
<Defs>

<!-- Immature Dryad -->

<RecipeDef>
    <defName>Make_ComponentDryadBasic</defName>
    <label>make immature essence</label>
    <description>Make an immature essence.</description>
    <jobString>Making immature essence.</jobString>
    <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_Machining</soundWorking>
    <workAmount>3000</workAmount>
    <unfinishedThingDef>UnfinishedComponent</unfinishedThingDef>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>Corpse_Dryad_Basic</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>WoodLog</li>
          </thingDefs>
        </filter>
        <count>20</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>Corpse_Dryad_Basic</li>
        <li>WoodLog</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <ComponentDryad_Basic>1</ComponentDryad_Basic>
    </products>
    <researchPrerequisite>Rootcrafting</researchPrerequisite>
    <skillRequirements>
      <Crafting>6</Crafting>
      <Plants>8</Plants>
    </skillRequirements>
    <workSkill>Plants</workSkill>
    <displayPriority>10</displayPriority>
  </RecipeDef>

