<?xml version="1.0" encoding="utf-8" ?>
<Defs>

<!-- Gaubricmaker Dryad -->

<RecipeDef MayRequire="vanillaexpanded.ideo.dryads">
    <defName>Make_ComponentDryadGaubricmaker</defName>
    <label>make fibrous essence</label>
    <description>Make a fibrous essence.</description>
    <jobString>Making fibrous essence.</jobString>
    <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_Machining</soundWorking>
    <workAmount>10000</workAmount>
    <unfinishedThingDef>UnfinishedComponent</unfinishedThingDef>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>Corpse_VDE_Dryad_Gaubricmaker</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>WoodLog</li>
          </thingDefs>
        </filter>
        <count>20</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>Corpse_VDE_Dryad_Gaubricmaker</li>
        <li>WoodLog</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <ComponentDryad_Firemaker>1</ComponentDryad_Firemaker>
    </products>
    <researchPrerequisite>Rootcraft</researchPrerequisite>
    <skillRequirements>
      <Crafting>6</Crafting>
      <Plants>8</Plants>
    </skillRequirements>
    <workSkill>Plants</workSkill>
    <displayPriority>10</displayPriority>
  </RecipeDef>

